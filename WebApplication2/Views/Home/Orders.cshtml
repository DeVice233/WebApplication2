@model IEnumerable<WebApplication2.Models.Order>
@{
    ViewBag.Title = "Все пользователи";
}
<table class="table">
      <tr> <th>Номер Заказа</th> <th>Id Пользователя</th><th>Id Услуги</th><th>Логин Пользователя</th><th>Название Услуги</th><th>Статус заказа</th><th>Время заказа</th></tr>
    @foreach (var item in Model)
    {
   
        <tr>
          <td>@item.Id</td>

    <td>@item.UserId</td>
    <td>@item.ServiceId</td>
  <td>@item.User.Name</td>
    <td>@item.Service.Name</td>  
 <td>@item.Status</td>
  <td>@item.OrderTime</td>
           </tr>
    }
</table>